{
  "name": "my-browser",
  "version": "1.0.0",
  "description": "基于 Electron 开发的浏览器",
  "main": "src/main.js",
  "scripts": {
    "start": "electron .",
    "dev": "env-cmd -f .env electron . --enable-logging --lang=zh-CN",
    "build": "electron-builder",
    "pack": "electron-builder --dir"
  },
  "keywords": [
    "electron",
    "browser",
    "web-browser"
  ],
  "author": "Your Name",
  "license": "MIT",
  "devDependencies": {
    "electron": "^28.0.0",
    "electron-builder": "^24.6.4",
    "env-cmd": "^10.1.0"
  },
  "build": {
    "appId": "com.whyun.my-browser",
    "productName": "My Browser",
    "directories": {
      "output": "dist"
    },
    "files": [
      "src/**/*",
      "node_modules/**/*"
    ]
  },
  "dependencies": {
    "node-slogger": "^3.0.0"
  }
}